APP_XDEBUG_MODE=debug,develop
APP_XDEBUG_CONFIG="client_host=host.docker.internal"
WWWUSER=1000
WWWGROUP=1000

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=daily
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
