APP_NAME="Laravel Starter"
APP_ENV=local
APP_KEY=

APP_DEBUG=true
APP_TIMEZONE=UTC
APP_PORT=8000
APP_URL="http://127.0.0.1:${APP_PORT}"

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
